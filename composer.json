{
    "name": "troy/cnvex",
    "description": "Cnvex is a PHP library for e.cnvex.cn",
    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "php": "^5.5.9 || ^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.1",
        "nesbot/carbon": "^1.20"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Troy",
            "email": "tangcy@cartechfin.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bravist\\Cnvex\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
     "extra": {
        "laravel": {
            "providers": [
                "Bravist\\Cnvex\\ServiceProvider"
            ],
            "aliases": {
                "Cnvex": "\\Bravist\\Cnvex\\Facade\\Cnvex",
                "CnvexSigner": "\\Bravist\\Cnvex\\Facade\\Signature"
            }
        }
    }
}
